<h1> Your Properties: </h1>
<br>

<table class="table table-striped" border="0" style="width:100%">
	<tr>
		<th>#</th>
		<th>Address One</th>
		<th>Address Two</th>
		<th>City</th>
		<th>State</th>
		<th>Zip code</th>
		<th>Monthly rent</th>
		<th>Action</th>
	</tr>

	<% @properties.each do |pro| %>

	<tr>
		<td><%= pro.id %></td>
		<td><%= pro.address_one %></td>
		<td><%= pro.address_two %></td>
		<td><%= pro.city %></td>
		<td><%= pro.state %></td>
		<td><%= pro.zip_code %></td>
		<td><%= pro.rent %></td>
		<td><%= link_to "Edit Property", pro %></td>
	</tr>

<br>
<% end %>
</table>

<a type="button" class="btn btn-primary btn-lg" href="properties/new">Add new property</a>